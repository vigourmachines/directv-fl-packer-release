{
  "name": "directv-fl",
  "version": "1.0.11",
  "description": "",
  "main": "index.js",
  "scripts": {
    "startHub": "./node_modules/vigour-hub/bin/hub -d -c directv",
    "build": "gaston build -s ./index.js -o . -i",
    "ios": "npm run build && ./node_modules/.bin/wrapper build --native.selectedPlatforms ios",
    "android": "npm run build && ./node_modules/.bin/wrapper build --native.selectedPlatforms android",
    "release": "ferry --release --mergeFiles '[\"./package.json\",\"./.package.json\"]'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/directv-fl.git"
  },
  "author": "Vigour",
  "bugs": {
    "url": "https://github.com/vigour-io/directv-fl/issues"
  },
  "keywords": [],
  "license": "MIT",
  "contributors": [],
  "engines": {},
  "homepage": "https://github.com/vigour-io/directv-fl",
  "dependencies": {
    "through2": "*",
    "lodash": "3.2.0",
    "monotonic-timestamp": "0.0.9",
    "promise": "^7.0.1",
    "old-vigour-js": "git+ssh://git@github.com:vigour-io/vigour-js.git#directv"
  },
  "devDependencies": {
    "vigour-env": "^1.0.3",
    "vigour-wrapper": "^2.0.1",
    "vigour-statusbar": "^1.0.4",
    "vigour-ferry": "^3.0.0",
    "vigour-hub": "git+ssh://git@github.com:vigour-io/vigour-hub.git#sbs"
  },
  "vigour": {
    "cloud": "https://vv-hub.dev.vigour.io",
    "development": {
      "button": false
    },
    "pointers": {
      "assets": "https://dtv-assets.vigour.io/"
    },
    "wrapper": {
      "assets": {
        "build.js": true,
        "build.css": true,
        "build.html": true,
        "assets": "*",
        "fonts.css": true
      }
    },
    "ferry": {
      "web": "build.html",
      "assets": {
        "build.js": true,
        "build.css": true,
        "build.html": true,
        "assets": "*",
        "fonts.css": true
      },
      "server": {}
    },
    "git": {
      "owner": "vigour-io",
      "repo": "directv-fl"
    },
    "mail": {
      "fromAddress": "shawn@vigour.io",
      "to": "shawn@vigour.io"
    },
    "slack": {},
    "native": {
      "platforms": {
        "ios": {
          "productName": "vanilla",
          "organizationName": "Vigour",
          "organizationIdentifier": "io.vigour.vanilla",
          "buildNumber": "1",
          "appIndexPath": "build.html",
          "appUrlIdentifier": "io.vigour.vanilla",
          "appUrlScheme": "vanilla",
          "appIcon": "assets/img/appicon.png",
          "splashScreen": "assets/img/splash.png",
          "assets": {
            "build.js": true,
            "build.css": true,
            "build.html": true,
            "assets": "*",
            "fonts.css": true
          }
        }
      }
    },
    "branches": {
      "youzi-dev": {
        "cloud": "http://Imac2014.local:10001",
        "pointers": {
          "img": "http://Imac2014.local/assets/",
          "assets": "http://Imac2014.local/assets/"
        }
      },
      "marcus-dev": {
        "cloud": "http://localhost:10001",
        "uscloud": "http://54.173.192.57:10001"
      },
      "shawn-dev": {
        "cloud": "http://192.168.2.18:10001"
      },
      "production": {
        "cloud": "http://52.8.23.45:80"
      },
      "sbs": {
        "cloud": "http://127.0.0.1:10001"
      },
      "stv": {
        "cloud": "http://stv-demo-hub.vigour.io/"
      },
      "adm": {
        "cloud": "http://localhost:10001"
      },
      "airplane": {
        "cloud": "http://52.8.40.195:80",
        "pointers": {
          "assets": "http://dtv-us1assets.vigour.io/"
        }
      },
      "ramon": {
        "cloud": "http://54.173.192.57:80/",
        "nobrand": true,
        "pointers": {
          "assets": "http://dtv-us1assets.vigour.io/"
        }
      },
      "vanilla-online": {
        "cloud": "https://vv-hub.dev.vigour.io",
        "pointers": {
          "assets": "https://dtv-assets.vigour.io/"
        }
      }
    }
  },
  "gaston": {
    "index-path": "index.html",
    "package": true,
    "less-options": {
      "strictMath": true
    },
    "transforms": {
      "package-branch-config": {
        "section": "vigour"
      }
    },
    "aliasify": {
      "dtv": "src",
      "shared": "src/components/shared",
      "dtv-components": "src/components",
      "dtv-active": "src/components/active",
      "dtv-item": "src/components/active/content-item",
      "dtv-channel": "src/components/active/content-item/channel",
      "dtv-episode": "src/components/active/content-item/episode",
      "dtv-movie": "src/components/active/content-item/movie"
    }
  }
}
