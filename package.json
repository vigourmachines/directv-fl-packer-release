          "assets": {
            "build.js": true,
            "build.css": true,
            "build.html": true,
            "assets": "*",
            "fonts.css": true
          }
        },
        "android": {
          "productName": "VIER+VIJF",
          "applicationId": "io.vigour.sbs",
          "appIndexPath": "build.html",
          "appIcon": "icon.png",
          "splashScreen": "splash.png",
          "splashDuration": 1500,
