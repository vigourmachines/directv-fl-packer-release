  "description": "",
  "main": "index.js",
  "scripts": {
    "startGaston": "./node_modules/.bin/gaston",
    "startHub": "./node_modules/vigour-hub/bin/hub -d -c directv",
    "prestart": "npm run startHub &",
    "start": "npm run startGaston",
    "test": "echo \"Error: no test specified\" && exit 1",
    "symlink": "mkdir -p node_modules; ln -sf $(pwd) $(pwd)/node_modules",
    "build": "./node_modules/.bin/gaston build",
    "ios": "npm run build && ./node_modules/.bin/vNative build -p ios",
    "android": "npm run build && ./node_modules/.bin/vNative build -p android",
    "samsungtv": "npm run build && ./node_modules/.bin/vNative build -p samsungtv",
    "all": "npm run build && ./node_modules/.bin/vNative build",
    "release": "packer -r -c package.json,.package.json"
  },
  "keywords": [

  ],
  "license": "",
  "contributors": [

  ],
  "engines": {

  },
