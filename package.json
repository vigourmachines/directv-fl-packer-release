  },
  "homepage": "https://github.com/vigour-io/directv-fl",
  "dependencies": {
    "lodash": "3.10.1",
    "monotonic-timestamp": "0.0.9",
    "package-branch-config": "^1.2.2",
    "promise": "^7.0.1",
    "through2": "*",
    "old-vigour-js": "git+ssh://git@github.com:vigour-io/vigour-js.git#directv",
    "vigour-status-bar": "^1.0.4"
  },
  "devDependencies": {
    "vigour-wrapper": "^2.1.3",
    "vigour-status-bar": "^1.0.5",
    "vigour-ferry": "^3.0.1",
