    "ferry": {
      "web": "build.html",
      "assets": {
        "build.js": true,
        "build.css": true,
        "build.html": true,
        "assets": "*",
        "fonts.css": true
      }
    },
    "git": {
      "owner": "vigour-io",
      "repo": "directv-fl"
    },
    "mail": {
